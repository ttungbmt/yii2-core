{
    "name": "ttungbmt/yii2-core",
    "description": "Core for Yii 2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension", "PHP"],
    "license": "MIT",
    "authors": [
        {
            "name": "Trương Thanh Tùng",
            "email": "ttungbmt@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap4": "@dev",
        "yiisoft/yii2-imagine": "*",
        "yiisoft/yii2-elasticsearch": "*",
        "yiisoft/yii2-authclient": "*",
        "yiisoft/yii2-httpclient": "*",
        "vlucas/phpdotenv": "*",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/http": "*",
        "kartik-v/yii2-widgets": "@dev",
        "kartik-v/yii2-export": "@dev",
        "kartik-v/yii2-validators": "@dev",
        "kartik-v/yii2-mpdf": "@dev",
        "kartik-v/yii2-builder": "@dev",
        "kartik-v/yii2-helpers": "@dev",
        "kartik-v/yii2-detail-view": "@dev",
        "kartik-v/yii2-date-range": "@dev",
        "kartik-v/yii2-field-range": "*",
        "kartik-v/yii2-tree-manager": "@dev",
        "kartik-v/yii2-datecontrol": "@dev",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "yii2mod/yii2-settings": "*",
        "sergeymakinen/yii2-facades": "^1.5",
        "nepstor/yii2-datetime-compare-validator": "*",
        "codeonyii/yii2-at-least-validator": "*",
        "mdmsoft/yii2-admin": "dev-master",
        "nanson/yii2-postgis": "^1.0"
    },
    "autoload": {
        "files": [
            "src/foundation/helpers.php"
        ],
        "psr-4": {
            "ttungbmt\\": "src"
        }
    },
    "extra": {
        "bootstrap": "ttungbmt\\base\\Bootstrap"
    }
}
